import java.util.*;
import java.io.*;

class Solution {
    public static void main(String []argh) {
        Scanner in = new Scanner(System.in);
        int t = in.nextInt();
        
        for(int i = 0; i < t; i++){
            int a = in.nextInt();
            int b = in.nextInt();
            int n = in.nextInt();
            
            int currentSum = a;
            for(int j = 0; j < n; j++) {
                // (1 << j) is a bitwise shift equivalent to 2^j
                // This is faster and more "Java-pro" than Math.pow
                currentSum += (1 << j) * b;
                
                System.out.print(currentSum + " ");
            }
            // Line break after each query series
            System.out.println();
        }
        in.close();
    }
}
